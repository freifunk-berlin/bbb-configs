#jinja2: trim_blocks: "true", lstrip_blocks: "true"
config server
    option port_start '{{ wireguard_port_start }}'
    option port_end '{{ wireguard_port_end }}'
    option base_prefix_ipv6 '{{ wireguard_base_prefix_ipv6 }}'
    {% if wireguard_base_prefix_ipv4 is defined %}
    option base_prefix_ipv4 '{{ wireguard_base_prefix_ipv4 }}'
    {% endif %}
    option wg_key '{{ wireguard_wg_key }}'
    option wg_pub '{{ wireguard_wg_pub }}'
    option wg_tmp_key '{{ wireguard_wg_tmp_key | int }}'
